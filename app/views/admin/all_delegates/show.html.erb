<%- model_class = AllDelegate -%>
<% title t '.title', :default => model_class.model_name.human %>

  <p>
    <strong><%= model_class.human_attribute_name(:first_name) %>:</strong>
    <%= @all_delegate.first_name %>
  </p>
  <p>
    <strong><%= model_class.human_attribute_name(:started_at) %>:</strong>
    <%= @all_delegate.started_at %>
  </p>
  <p>
    <strong><%= model_class.human_attribute_name(:ended_at) %>:</strong>
    <%= @all_delegate.ended_at %>
  </p>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_all_delegates_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_all_delegate_path(@all_delegate), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_all_delegate_path(@all_delegate),
              :method => :delete,
							:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
	<%= link_to t('helpers.links.admin'), admin_path,
  :class => 'btn' %>
</div>
